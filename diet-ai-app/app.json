{
  "expo": {
    "name": "diet-ai-app",
    "slug": "diet-ai-app",
    "owner": "hwak8459",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "dietaiapp",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "jsEngine": "hermes",
      "edgeToEdgeEnabled": true,
      "package": "com.psk00.dietaiapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "entryPoint": "./node_modules/expo-router/entry",
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f1605568-274e-4401-ae01-9d3843cf2307"
      }
    }
  }
}
